{
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": { "legacyDecorator": true, "decoratorMetadata": true },
    "keepClassNames": true
  },
  "module": { "type": "commonjs" },
  "sourceMaps": true
}
